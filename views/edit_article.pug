extends layout

block content
  .col-md-12.center.float-none
    h1 Submit Self Review
    form(method='POST', action='/articles/edit/'+article._id)
      .form-group.self-review
        .row
          .col-md-1.form-label-col(data-toggle="tooltip" title="How we work with peers & customers and role model our values")
            label.form-title Teamwork:
            br
          .col-md-11.form-info-col
            .form-check.self-review-check.self-review-check-first
              input.form-check-input(type='radio', name='teamwork', value='Unsatisfactory' id=article.teamwork)
              label.form-check-label.label-form(for='exampleRadios1')
                | Unsatisfactory
            .form-check.self-review-check
              input.form-check-input(type='radio', name='teamwork', value='Needs Improvement' id=article.teamwork)
              label.form-check-label.label-form(for='exampleRadios1')
                | Needs Improvement
            .form-check.self-review-check
              input.form-check-input(type='radio', name='teamwork', value='Meets Expectations' id=article.teamwork)
              label.form-check-label.label-form(for='exampleRadios1')
                | Meets Expectations
            .form-check.self-review-check
              input.form-check-input(type='radio', name='teamwork', value='Exceeds Expectations' id=article.teamwork)
              label.form-check-label.label-form(for='exampleRadios1')
                | Exceeds Expectations
            .form-check.self-review-check
              input.form-check-input(type='radio', name='teamwork', value='outstanding' id=article.teamwork)
              label.form-check-label.label-form(for='exampleRadios1')
                | Outstanding
      .form-group.self-review
        .row
          .col-md-1.form-label-col(data-toggle="tooltip" title="How we deliver high quality results")
            label.form-title Results:
            br
          .col-md-11.form-info-col
            .form-check.self-review-check.self-review-check-first
              input.form-check-input(type='radio', name='results', value='Unsatisfactory')
              label.form-check-label.label-form(for='exampleRadios1')
                | Unsatisfactory
            .form-check.self-review-check
              input.form-check-input(type='radio', name='results', value='Needs Improvement')
              label.form-check-label.label-form(for='exampleRadios1')
                | Needs Improvement
            .form-check.self-review-check
              input.form-check-input(type='radio', name='results', value='Meets Expectations')
              label.form-check-label.label-form(for='exampleRadios1')
                | Meets Expectations
            .form-check.self-review-check
              input.form-check-input(type='radio', name='results', value='Exceeds Expectations')
              label.form-check-label.label-form(for='exampleRadios1')
                | Exceeds Expectations
            .form-check.self-review-check
              input.form-check-input(type='radio', name='results', value='outstanding')
              label.form-check-label.label-form(for='exampleRadios1')
                | Outstanding
      .form-group.self-review
        .row
