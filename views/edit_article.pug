                | Exceeds Expectations
            .form-check.self-review-check
              input.form-check-input(type='radio', name='development', value='outstanding')
              label.form-check-label.label-form(for='exampleRadios1')
                | Outstanding
      .form-group.self-review
        .row
          .col-md-1.form-label-col
            label.form-title Overall Performance Rating:
            br
          .col-md-11.form-info-col
            .form-check.self-review-check.self-review-check-first
              input.form-check-input(type='radio', name='overallResult', value='Unsatisfactory')
              label.form-check-label.label-form(for='exampleRadios1')
                | Unsatisfactory
            .form-check.self-review-check
              input.form-check-input(type='radio', name='overallResult', value='Needs Improvement')
              label.form-check-label.label-form(for='exampleRadios1')
                | Needs Improvement
            .form-check.self-review-check
              input.form-check-input(type='radio', name='overallResult', value='Meets Expectations')
              label.form-check-label.label-form(for='exampleRadios1')
                | Meets Expectations
            .form-check.self-review-check
              input.form-check-input(type='radio', name='overallResult', value='Exceeds Expectations')
              label.form-check-label.label-form(for='exampleRadios1')
                | Exceeds Expectations
            .form-check.self-review-check
              input.form-check-input(type='radio', name='overallResult', value='outstanding')
              label.form-check-label.label-form(for='exampleRadios1')
                | Outstanding
      .form-group
        label.form-title Any Comments:
        br
        textarea.form-control(name='comments')=article.comments
      br
      input.btn.btn-primary(type='submit', value='Submit' style="margin-left:15px;")

  script.
    $(document).ready(function(){
      $('[data-toggle="tooltip"]').tooltip();
    });



